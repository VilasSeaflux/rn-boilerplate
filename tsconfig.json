{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"jsx": "react-jsx",
		"strict": true,
		"baseUrl": "./src",
		"paths": {
			"@components/*": ["components/*"],
			"@providers/*": ["providers/*"],
			"@screens/*": ["screens/*"],
			"@utils/*": ["utils/*"],
			"@assets/*": ["../assets/*"],
			"@hooks/*": ["hooks/*"],
			"@constants/*": ["constants/*"],
			"@navigation/*": ["navigation/*"],
			"@types/*": ["types/*"],
			"@services/*": ["services/*"],
			"@store/*": ["store/*"],
			"@styles/*": ["styles/*"],
		}
	}
}
